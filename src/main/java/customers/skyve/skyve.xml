<?xml version="1.0" encoding="UTF-8"?>
<customer xmlns="http://www.skyve.org/xml/customer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="skyve" xsi:schemaLocation="http://www.skyve.org/xml/customer ../../schemas/customer.xsd">
    <uiResources logo="logo.png"/>
    <defaultDateConverter>DD_MMM_YYYY</defaultDateConverter>
    <defaultTimeConverter>HH24_MI</defaultTimeConverter>
    <defaultDateTimeConverter>DD_MMM_YYYY_HH24_MI</defaultDateTimeConverter>
    <defaultTimestampConverter>DD_MMM_YYYY_HH24_MI_SS</defaultTimestampConverter>
    <modules homeModule="workflowTestbed">
        <module name="admin"/>
        <module name="workflowTestbed"/>
    </modules>
    <textSearchRoles>
        <role module="admin" name="BasicUser"/>
    </textSearchRoles>
    <flagRoles>
        <role module="admin" name="BasicUser"/>
    </flagRoles>
    <switchModeRoles>
        <role module="admin" name="BasicUser"/>
    </switchModeRoles>
    <interceptors>
        <interceptor className="modules.admin.RDBMSAuditInterceptor"/>
    </interceptors>
</customer>